{
  "name": "injectar",
  "version": "1.2.1",
  "description": "Inject a fake http request to test, debug your server logic with ease.",
  "main": "src/index.js",
  "scripts": {
    "coverage": "npm run unit -- --cov --coverage-report=html",
    "lint": "standard --fix src/*.js | snazzy",
    "test": "npm run lint && npm run unit",
    "unit": "tap test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fralonra/injectar.git"
  },
  "keywords": [
    "http",
    "inject",
    "fake",
    "request"
  ],
  "author": "zoron (https://github.com/fralonra/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fralonra/injectar/issues"
  },
  "homepage": "https://github.com/fralonra/injectar#readme",
  "dependencies": {
    "light-my-request": "^3.6.0"
  },
  "devDependencies": {
    "snazzy": "^8.0.0",
    "standard": "^14.2.0",
    "tap": "^14.6.2"
  }
}
